@model ProductImageDto
@{
    ViewData["Title"] = "ShowImage";
}



<div class="col-sm-6 col-xl-3">
    <div class="card">
        <img class="card-img-top img-fluid" src="~/user/images/@Model.Image" alt="Card image cap">
        <div class="card-body">
            <div class="text-end">
                <a class="btn btn-secondary text-white mr-2" asp-action="Index">Go Back</a>
            </div>
        </div>
    </div>
</div>